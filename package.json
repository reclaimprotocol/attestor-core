{
  "name": "@reclaimprotocol/witness-sdk",
  "version": "2.0.2",
  "description": "",
  "main": "lib/index",
  "scripts": {
    "build": "npm exec tsc",
    "build:browser": "sh ./src/scripts/build-browser.sh",
    "start:tsc": "ts-node src/scripts/start-server",
    "start": "node lib/scripts/start-server",
    "prepare": "sh ./src/scripts/prepare.sh",
    "test": "NODE_ENV=test TZ=utc jest --verbose --forceExit --detectOpenHandles",
    "test:avs": "NODE_ENV=test TZ=utc jest --verbose --forceExit --detectOpenHandles --testPathPattern=avs",
    "lint": "eslint ./ --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint ./ --fix --ext .js,.ts,.jsx,.tsx",
    "generate:proto": "sh ./src/scripts/generate-proto.sh",
    "generate:contracts-data": "sh ./src/scripts/contract-data-gen.sh",
    "generate:provider-types": "ts-node src/scripts/generate-provider-types.ts",
    "verify:root-ca": "ts-node src/scripts/verify-root-ca.ts",
    "create:claim": "ts-node src/scripts/generate-receipt.ts",
    "build-contracts": "cd avs/contracts && forge build",
    "deploy:contracts": "sh avs/utils/anvil/deploy-all-to-anvil-and-save-state.sh",
    "deploy:contracts-to-chain": "sh avs/utils/anvil/deploy-to-chain.sh",
    "register:avs-operator": "ts-node src/scripts/register-avs-operator.ts",
    "check:avs-registration": "ts-node src/scripts/check-avs-registration.ts",
    "generate:avs": "typechain -- --target ethers-v5 --out-dir src/avs/contracts avs/contracts/out/ReclaimServiceManager.sol/*.json",
    "start:chain": "sh ./avs/utils/anvil/start-anvil-chain-with-el-and-avs-deployed.sh"
  },
  "files": [
    "lib/*"
  ],
  "keywords": [],
  "author": "Adhiraj Singh",
  "license": "ISC",
  "dependencies": {
    "@bufbuild/protobuf": "^2.0.0",
    "@reclaimprotocol/circom-symmetric-crypto": "git+https://gitlab.reclaimprotocol.org/reclaim/zk-symmetric-crypto",
    "@reclaimprotocol/tls": "git+https://gitlab.reclaimprotocol.org/reclaim/tls",
    "ajv": "^8.17.1",
    "canonicalize": "^2.0.0",
    "dotenv": "^16.4.5",
    "elastic-apm-node": "^4.7.3",
    "esprima-next": "^5.8.4",
    "ethers": "^5.7.2",
    "https-proxy-agent": "^7.0.5",
    "jsdom": "^24.1.3",
    "jsonpath-plus": "^9.0.0",
    "koffi": "^2.9.0",
    "p-queue": "^8.0.1",
    "pino": "^9.4.0",
    "protobufjs": "^7.4.0",
    "re2": "^1.21.4",
    "serve-static": "^1.15.0",
    "snarkjs": "^0.7.4",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@adiwajshing/eslint-config": "github:adiwajshing/eslint-config",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.7.23",
    "@swc/jest": "^0.2.36",
    "@typechain/ethers-v5": "^11.1.2",
    "@types/chance": "^1.1.6",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.16.5",
    "@types/serve-static": "^1.15.7",
    "@types/ws": "^8.5.12",
    "chance": "^1.1.12",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "json-schema-to-typescript": "^15.0.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "ts-proto": "^2.2.0",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "yaml": "^2.5.1"
  },
  "overrides": {
    "@ethersproject/providers": {
      "ws": "^7.5.10"
    },
    "elliptic": "^v6.5.7"
  }
}
